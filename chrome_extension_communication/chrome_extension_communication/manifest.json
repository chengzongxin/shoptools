{
  "manifest_version": 3,
  "name": "WebSocket通信Demo",
  "version": "1.0.1",
  "description": "与本地Python程序通过WebSocket通信的Demo",
  "permissions": [
    "scripting",
    "cookies",
    "tabs",
    "webRequest",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "WebSocket通信Demo",
    "default_popup": "popup.html"
  }
}